// AXIS Example 08: Boolean Logic
// Demonstrates True/False and boolean negation
// Expected exit code: 42

func main() -> i32:
    # Boolean variables
    is_ready: bool = True
    is_blocked: bool = False
    
    # Boolean negation
    can_proceed: bool = !is_blocked  # True
    
    when is_ready:
        when can_proceed:
            give 42  # Success
    
    give 0
